{
    "name": "reciple-dev-commands",
    "description": "Add Reciple dev only commands",
    "version": "2.1.2",
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/esm.mjs",
    "types": "./dist/index.d.ts",
    "recipleModule": "./dist/module.mjs",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/esm.mjs",
            "require": "./dist/index.js"
        },
        "./module": {
            "import": "./dist/module.mjs",
            "types": "./dist/module.d.mts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "keywords": [
        "reciple-module"
    ],
    "scripts": {
        "clean": "npx rimraf ./dist",
        "build": "npm run clean && npx tsc --noEmitOnError true"
    },
    "dependencies": {
        "fallout-utility": "^2.9.1"
    },
    "devDependencies": {
        "reciple-registry-cache": "^2.0.0-dev.0"
    },
    "peerDependencies": {
        "@reciple/core": "^9 || ^9.0.0-dev"
    },
    "peerDependenciesMeta": {
        "@reciple/core": {
            "optional": true
        }
    },
    "optionalDependencies": {
        "reciple-registry-cache": "^2.1.2"
    }
}
